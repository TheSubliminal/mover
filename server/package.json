{
  "name": "mover",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "lint": "eslint --fix ./**/*.js",
    "migrate": "npx knex migrate:latest",
    "migrate:reset": "npx knex migrate:rollback --all && npm run migrate && npm run seed",
    "seed": "npx knex seed:run",
    "rollback": "npx knex migrate:rollback",
    "pretest": "npx cross-env NODE_ENV=test npm run migrate:reset",
    "test": "npx cross-env NODE_ENV=test jest --testTimeout=10000",
    "coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/multer": "^2.0.0",
    "@koa/router": "^8.0.1",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cross-env": "^6.0.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.5",
    "koa": "^2.8.2",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^4.0.0",
    "koa-passport": "^4.1.3",
    "koa-static": "^5.0.0",
    "multer": "^1.4.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "supertest": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
